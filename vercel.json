{
  "version": 2,
  "name": "whatsapp-calling-widget",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "env": {
    "REACT_APP_API_URL": "https://call-uat.tgrnd.com",
    "REACT_APP_WEBSOCKET_URL": "https://call-uat.tgrnd.com",
    "REACT_APP_DEV_MODE": "false",
    "REACT_APP_TWILIO_WHATSAPP_NUMBER": "whatsapp:+447366331247"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
